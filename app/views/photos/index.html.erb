<p id="notice"><%= notice %></p>

<h1>Listing Photos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      
       <tr>
       <td><%= image_tag photo.meetuppic.url %></td>
       <td><%= link_to 'Destroy',  meetup_photo_path(@meetup ,photo) ,method: :delete , data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_meetup_photo_path(@meetup) %>
